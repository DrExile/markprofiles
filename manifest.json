{
  "manifest_version": 3,
  "name": "Profile Highlighter",
  "version": "1.0",
  "description": "Highlight marked profiles with red color",
  "permissions": ["storage"],
  "host_permissions": ["https://*.pathofexile.com/*"],
  "content_scripts": [
    {
      "matches": ["https://*.pathofexile.com/*"],
      "js": ["content.js"]
    }
  ]
}
