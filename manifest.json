{
  "manifest_version": 3,
  "name": "Highlight Profile Links",
  "version": "1.0",
  "description": "Выделяет текст в формате 'Enryu#9023' красным цветом",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
